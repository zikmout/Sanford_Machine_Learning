define("js/lib/useragent",["require","exports","module","js/app/polyfills"],function(require,exports,module){"use strict";function browserNameIn(e){var i=["Edge","PhantomJS","Chrome","IE","Firefox","Safari","Konqueror","Opera","Googlebot"];return i.find(function(i){return t[i].test(e)})||"unknown"}function browserVersionIn(e){switch(browserNameIn(e)){case"Chrome":if(n.Chrome.test(e))return RegExp.$1;break;case"Safari":if(n.Safari.test(e))return RegExp.$1;break;case"Firefox":if(n.Firefox.test(e))return RegExp.$1;break;case"IE":if(n.IE.test(e))return RegExp.$1;if(n.IE11.test(e))return RegExp.$1;break;case"Edge":if(n.Edge.test(e))return RegExp.$1;break;case"PS3":if(n.Ps3.test(e))return RegExp.$1;break;case"PSP":if(n.Psp.test(e))return RegExp.$1;break;default:var i=/#\{name\}[\/ ]([\d\w\.\-]+)/i;if(i.test(e))return RegExp.$1}}function systemIn(i){return e.WindowsVista.test(i)?"Windows Vista":e.Windows8.test(i)?"Windows 8":e.Windows7.test(i)?"Windows 7":e.Windows2003.test(i)?"Windows 2003":e.WindowsXP.test(i)?"Windows XP":e.Windows2000.test(i)?"Windows 2000":e.Linux.test(i)?"Linux":e.OSX.test(i)?i.match(e.OSX)[0].replace("_","."):e.Ipad.test(i)?i.match(e.Ipad)[0].replace("_",".").replace("(",""):e.Iphone.test(i)?i.match(e.Iphone)[0].replace("_",".").replace("(",""):"unknown"}function platformIn(e){return i.Windows.test(e)?"Microsoft Windows":i.Mac.test(e)?"Apple Mac":i.Android.test(e)?"Android":i.Blackberry.test(e)?"Blackberry":i.Linux.test(e)?"Linux":i.Ipad.test(e)?"iPad":i.Ipod.test(e)?"iPod":i.Iphone.test(e)?"iPhone":"unknown"}function UserAgent(){var e=arguments.length>0&&void 0!==arguments[0]?arguments[0]:navigator.userAgent,n=/iPhone|iPod|iPad|Android|BlackBerry|Opera Mini|IEMobile/.test(e),a=!!e.match(r),u=browserNameIn(e),p=browserVersionIn(e),i=platformIn(e),c=systemIn(e),w=!!e.match(s),t=void 0,d=void 0;"undefined"!=typeof document&&"undefined"!=typeof window?"undefined"!=typeof window.screen?(t=window.screen.height,d=window.screen.width):(t=window.innerHeight,d=window.innerWidth):(t=0,d=0);var f={source:e,browser:{name:u,version:p},system:c,platform:i,isMobileBrowser:n,isAndroid:n&&"Android"===i,isCourseraMobileApp:a,isMobile:n||a,isIOS:n&&("iPad"===i||"iPhone"===i||"iPod"===i),isMobilePhoneBrowser:n&&(420>t||420>d),isPrerender:w,isRobot:w||!!e.match(o)};return f}require("js/app/polyfills");var o=/PhantomJS|Googlebot/,s=/Prerender/,r=/Coursera-Mobile/,n={Firefox:/Firefox\/([\d\w\.\-]+)/,IE:/MSIE\s([\d\.]+[\d])/,IE11:/Trident.*rv\:(11)\./,Chrome:/(?:Chrome|CriOS)\/([\d\w\.\-]+)/,Safari:/Version\/([\d\w\.\-]+)/,Ps3:/([\d\w\.\-]+)\)\s*$/,Psp:/([\d\w\.\-]+)\)?\s*$/,Edge:/Edge\/([\d\.]+)/},t={Chrome:/Chrome|CriOS/,Firefox:/Firefox/,PhantomJS:/PhantomJS/,Googlebot:/Googlebot/,IE:/MSIE|Trident.*rv\:11\./,Konqueror:/Konqueror/,Opera:/Opera/,Safari:/Safari/,Edge:/Edge/},e={Windows2000:/Windows NT 5\.0/,WindowsXP:/Windows NT 5\.1/,Windows2003:/Windows NT 5\.2/,WindowsVista:/Windows NT 6\.0/,Windows7:/Windows NT 6\.1/,Windows8:/Windows NT 6\.[2-9]/,OSX:/Mac OS X (\d+)[._](\d+)/,Linux:/Linux/i,Ipad:/\(iPad.*OS (\d+)[._](\d+)/,Iphone:/\(iPhone.*OS (\d+)[._](\d+)/},i={Windows:/Windows/,Mac:/Macintosh/,Linux:/Linux/,Ipad:/iPad/,Ipod:/iPod/,Iphone:/iPhone/,Android:/Android/,Blackberry:/Blackberry/};module.exports=UserAgent});